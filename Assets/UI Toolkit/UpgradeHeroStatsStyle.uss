.button {
    background-color: rgb(40, 149, 43);
    color: white;
    border-radius: 6px;
    padding: 6px 12px;
    border-width: 0;
    font-size: 14px;
    font-weight: bold;
    justify-content: center;
    align-items: center;
    transition-property: background-color, transform;
    transition-duration: 100ms;
    cursor: pointer;
}

.label {
    background-color: rgb(156, 46, 46);
    align-self: auto;
    color: rgb(255, 255, 255);
    -unity-font-style: bold;
}
